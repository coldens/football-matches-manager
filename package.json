  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "type": "commonjs",
  "dependencies": {
    "@nestjs/common": "^10.4.1",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.1",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/platform-express": "^10.4.1",
    "@prisma/client": "5.19.1",
    "class-transformer": "^0.5.1",
